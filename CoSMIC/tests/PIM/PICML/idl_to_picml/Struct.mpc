// -*- MPC -*-
// $Id$

project (Struct_Expected) : taoclient {
  exename = Struct_Expected
  install = .

  Source_Files {
    Struct.cpp
  }

  IDL_Files {
    Struct.idl
  }
}

project (Struct_IDLTOPICML) : idl2picml {
  custom_only = 1

  after += Struct_Expected

  IDL2PICML_Files {
    Struct.idl
  }
}

project (Struct) : taoclient {
  exename = Struct
  install = .

  after += Struct_IDLTOPICML

  Source_Files {
    Struct.cpp
  }

  IDL_Files {
    gendir = .
    ./generated/Struct.idl
  }
}
