// $Id$

workspace {
  cmdline += -feature_file default.features \
             -include $COSMIC_ROOT/MPC/config

  ./picml.mwc
  
  exclude (!prop:windows) {
    PIM/SIML/interpreters/Interpreters.mwc
    PIM/POSAML/interpreters/interpreters.mwc
    PSM/CQML/interpreters/Interpreters.mwc
    PSM/DQML

    static {
      cmdline += -static \
                 -include $COSMIC_ROOT/contrib/CCF/MPC/config

      $(COSMIC_ROOT)/contrib/CCF/CCF/CodeGenerationKit
    }
  }
}
