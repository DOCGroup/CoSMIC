project (CQML) : cosmic_common, udm, cosmic_utils {

  UDM_Files {
      gendir = .
      $(COSMIC_ROOT)/PIM/CQML/paradigms/CQML.xml
      commandflags      = -l CQML_Export -v
   }

   after += CoSMIC_Utils
   sharedname = CQML
   macros += CQML_HAS_DLL CQML_EXPORT

   Source_Files {
     CQML.cpp
   }

   Header_Files {
     CQML.h
     CQML_Export.h
   }
}

