// MPC file to generate library
project {
   libout = $(PICML_ROOT)/lib
   dllout = $(PICML_ROOT)/bin
   includes += $(GME_ROOT)/SDK/BON \
               $(UDM_PATH)/include \
               $(UDM_PATH)/3rdParty/stl
   macros += UDM_DLL_EXPORT UDM_DYNAMIC_LINKING
   libpaths += $(UDM_PATH)/lib \
               $(UDM_PATH)/3rdParty/zlib \
               $(UDM_PATH)/3rdParty/xerces/xerces-c2_2_0-win32/lib
}
