// $Id$

project (PICML): cosmic_common, udm, xerces, cosmic_utils {

   UDM_Files {
      gendir = .
      $(COSMIC_ROOT)/PIM/PICML/paradigms/PICML.xml
      commandflags      = -l PICML_Export -v
   }

   sharedname = PICML

   libs  += GME
   after += GME

   macros += PICML_HAS_DLL PICML_EXPORT

   Source_Files {
     Data_Value_Builder.cpp
     PICML.cpp
   }

   Header_Files {
     PICML_Export.h
   }
}
