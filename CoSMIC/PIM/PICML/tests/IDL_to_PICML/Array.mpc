// -*- MPC -*-
// $Id: $

project (Array_Expected) : taoclient {
  exename = Array_Expected
  install = .

  Source_Files {
    Array.cpp
  }

  IDL_Files {
    Array.idl
  }
}

project (Array_IDLTOPICML) : idl2picml {
  custom_only = 1

  after += Array_Expected

  IDL2PICML_Files {
    Array.idl
  }
}

project (Array) : taoclient {
  exename = Array
  install = .

  after += Array_IDLTOPICML

  Source_Files {
    Array.cpp
  }

  IDL_Files {
    gendir = .
    ./generated/Array.idl
  }
}
