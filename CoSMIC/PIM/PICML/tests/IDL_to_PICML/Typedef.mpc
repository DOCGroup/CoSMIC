// -*- MPC -*-
// $Id: $

project (Typedef_Expected) : taoclient {
  exename = Typedef_Expected
  install = .

  Source_Files {
    Typedef.cpp
  }

  IDL_Files {
    Typedef.idl
  }
}

project (Typedef_IDLTOPICML) : idl2picml {
  custom_only = 1

  after += Typedef_Expected

  IDL2PICML_Files {
    Typedef.idl
  }
}

project (Typedef) : taoclient {
  exename = Typedef
  install = .

  after += Typedef_IDLTOPICML

  Source_Files {
    Typedef.cpp
  }

  IDL_Files {
    gendir = .
    ./generated/Typedef.idl
  }
}
