// -*- MPC -*-
// $Id$

project (Constant_IDLTOPICML) : idl2picml {
  custom_only = 1
  
  IDL2PICML_Files {
    Constant.idl
  }
}

project (Constant_IDL_Gen) : taoidldefaults {
  custom_only = 1
  after += Constant_IDLTOPICML
  
  IDL_Files {
    ./generated/Constant.idl
  }
}

project (Constant) : taoclient {
  exename = Constant
  install = .
  
  after += Constant_IDL_Gen
    
  Source_Files {
    Constant.cpp
  }
  
  IDL_Files {
  
  }
}
