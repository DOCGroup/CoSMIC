// MPC file to generate base library
project : mfc, cosmic_common, cosmic_utils {
  includes += $(GME_ROOT)/SDK/BON
  macros += BUILDER_OBJECT_NETWORK_V2

  specific {
     midl_flags += /server none /client none
     midl_includes += $(GME_ROOT) $(GME_ROOT)/Interfaces .
   }

  specific(nmake, vc71, vc8) {
    compile_flags += /wd4251
  }
}
