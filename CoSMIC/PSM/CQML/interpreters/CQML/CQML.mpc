project (CQML) : cosmic_common, udm, xerces, cosmic_utils {
  requires += xerces2

  UDM_Files {
      gendir = .
      $(COSMIC_ROOT)/PSM/CQML/paradigms/CQML.xml
      commandflags      = -l CQML_Export -v
   }

   sharedname = CQML
   macros += CQML_HAS_DLL CQML_EXPORT

   Source_Files {
     CQML.cpp
   }

   Header_Files {
     CQML.h
     CQML_Export.h
     Acceptor.h
   }
}
