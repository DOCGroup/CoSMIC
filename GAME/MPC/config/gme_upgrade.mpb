// $Id$

project {
  Define_Custom (Upgrade) {
    automatic          = 1
    command            = <%quote%>$(GAME_ROOT)/bin/gme_upgrade<%quote%>
    inputext           = .xme
    generic_outputext  = .mga
    output_option      = -o

    keyword gme_upgradeflags = commandflags
  }

  requires += gme_upgrade
}
